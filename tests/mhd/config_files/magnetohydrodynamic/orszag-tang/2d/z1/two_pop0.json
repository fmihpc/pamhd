{
	"comment": "two population version of hll_athena.json, pop1 on one side pop2 on other",
	"output-directory": "results/magnetohydrodynamic/orszag-tang/2d/z1/2hll_athena0",
	"time-start": 0,
	"time-length": 5,
	"solver-mhd": "hll-athena",
	"load-balancer": "RANDOM",
	"save-mhd-n": 0.5,
	"adiabatic-index": 1.666666667,
	"vacuum-permeability": 1,
	"proton-mass": 1,
	"mhd-time-step-factor": 0.5,
	"grid-options": {
		"periodic": "{true, true, false}",
		"cells": "{20, 20, 1}",
		"volume": "{2*pi, 2*pi, 1}",
		"start": "{0, 0, 0}"
	},
	"number-density": {"default": "x < pi ? 2.777778 : 0"},
	"number-density2": {"default": "x < pi ? 0 : 2.777778"},
	"velocity": {"default": "{-sin(y), sin(x), 0}"},
	"velocity2": {"default": "{-sin(y), sin(x), 0}"},
	"pressure": {"default": 1.666667, "comment": "ignored when mass == 0"},
	"pressure2": {"default": 1.666667},
	"magnetic-field": {"default": "{-sin(y), sin(2*x), 0}"}
}
