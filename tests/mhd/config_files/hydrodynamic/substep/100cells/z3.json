{
	"output-directory": "tests/mhd/results/hydrodynamic/substep/100cells/z3",
	"time-start": 0,
	"time-length": 1,
	"solver-mhd": "rusanov",
	"load-balancer": "RANDOM",
	"save-mhd-n": 0,
	"adiabatic-index": 1.6666666667,
	"vacuum-permeability": 1,
	"proton-mass": 1,
	"mhd-time-step-factor": 0.5,
	"substep-min" : "abs(z - 0.2) < 0.1 ? 0 : 1",
	"substep-max" : "abs(z - 0.2) < 0.1 ? 0 : 1",
	"grid-options": {
		"periodic": "{true, true, true}",
		"cells": "{1, 1, 50}",
		"volume": "{1, 1, 1}",
		"start": "{0, 0, 0}",
		"max-ref-lvl": 1,
		"amr-n": 0,
		"ref-lvl-at-least": "abs(z - 0.2) < 0.1 ? 1 : 0",
		"ref-lvl-at-most": "abs(z - 0.2) < 0.1 ? 1 : 0"
	},
	"number-density": {"default": "abs(z - 0.5) < 0.25 ? 2 : 1"},
	"velocity": {"default": [1, 0, 0]},
	"pressure": {"default": 1},
	"magnetic-field": {"default": [0, 0, 0]}
}
